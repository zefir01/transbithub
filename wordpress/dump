--
-- Table structure for table `wp_pprh_table`
--

DROP TABLE IF EXISTS `wp_pprh_table`;
/*!40101 SET @saved_cs_client     = @@character_set_client */;
/*!40101 SET character_set_client = utf8 */;
CREATE TABLE `wp_pprh_table` (
  `id` int(9) NOT NULL AUTO_INCREMENT,
  `url` varchar(255) COLLATE utf8mb4_unicode_ci NOT NULL DEFAULT '',
  `hint_type` enum('dns-prefetch','prefetch','prerender','preconnect','preload') COLLATE utf8mb4_unicode_ci NOT NULL,
  `status` varchar(55) COLLATE utf8mb4_unicode_ci NOT NULL DEFAULT 'enabled',
  `as_attr` varchar(55) COLLATE utf8mb4_unicode_ci DEFAULT '',
  `type_attr` varchar(55) COLLATE utf8mb4_unicode_ci DEFAULT '',
  `crossorigin` varchar(55) COLLATE utf8mb4_unicode_ci DEFAULT '',
  `media` varchar(255) COLLATE utf8mb4_unicode_ci DEFAULT '',
  `created_by` varchar(55) COLLATE utf8mb4_unicode_ci NOT NULL DEFAULT '',
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=42 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;
/*!40101 SET character_set_client = @saved_cs_client */;

